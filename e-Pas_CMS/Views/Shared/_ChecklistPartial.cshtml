@using e_Pas_CMS.ViewModels
@using Newtonsoft.Json
@model List<AuditChecklistNode>

@{
    bool isVerified = ViewBag.IsVerified ?? false;
    bool isReadonlyUser = ViewBag.IsReadonlyUser ?? false;
    string auditId = ViewBag.AuditId ?? "";
}

<ul id="auditChecklist" class="list-unstyled">
    @foreach (var node in Model)
    {
        await Html.RenderPartialAsync("_ChecklistNode", node);
    }
</ul>
